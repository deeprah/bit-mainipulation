// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
    int n;
    cout<<"enter the value of integer"<<endl;
    cin>>n;
    if((n &(n-1))==0)
    {
        cout<<"yes it is in power of two"<<endl;
    }
    else
    {
        cout<<"no";
    }
    

    return 0;
}
